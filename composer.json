{
  "name": "fruitcake/magento2-debugbar",
  "description": "Debugbar for Magento 2",
  "keywords": ["magento", "magento2", "toolbar", "debugbar", "profiler", "debug", "dev"],
  "type": "magento2-module",
  "authors": [
    {
      "name": "Fruitcake",
      "homepage": "https://fruitcake.nl"
    },
    {
      "name": "Barry vd. Heuvel"
    }
  ],
  "license": [
    "OSL-3.0"
  ],
  "require": {
    "php": "^8.1",
    "magento/framework": "*",
    "php-debugbar/php-debugbar": "~2.1.3"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://repo.magento.com/"
    }
  ],
  "autoload": {
    "psr-4": {
      "Fruitcake\\MagentoDebugbar\\": ""
    },
    "files": [
      "registration.php",
      "helpers.php"
    ]
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.1-dev"
    }
  },
  "config": {
    "allow-plugins": {
      "magento/composer-dependency-version-audit-plugin": true
    }
  }
}
